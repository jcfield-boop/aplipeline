{
  "name": "APLCICD",
  "version": "2.0.0",
  "description": "Vibe Coding & Self-Improving APL CI/CD System with Git Integration",
  "author": "APL Forge Contest 2025",
  "license": "MIT",
  "dyalog_version": "19.0",
  "main": "./src/APLCICD.dyalog",
  "startup": "APLCICD.Initialize",
  "target": "1",
  "dependencies": {
    "Conga": "*",
    "HttpCommand": "*"
  },
  "optional_dependencies": {
    "JSONlib": "*"
  },
  "exports": {
    "APLCICD": "./src/APLCICD.dyalog",
    "APLCICD.Core": "./src/Core.dyalog",
    "APLCICD.Pipeline": "./src/Pipeline.dyalog",
    "APLCICD.Monitor": "./src/Monitor.dyalog",
    "APLCICD.Config": "./src/Config.dyalog",
    "APLCICD.GitAPL": "./src/GitAPL.dyalog",
    "APLCICD.WebServer": "./src/WebServer.dyalog",
    "APLCICD.Vibe": "./src/vibe.dyalog",
    "APLCICD.SelfOptimizer": "./src/SelfOptimizer.dyalog",
    "APLCICD.APLPatterns": "./src/APLPatterns.dyalog",
    "APLCICD.Tests": "./src/Tests.dyalog",
    "APLCICD.RecursiveTest": "./src/RecursiveTest.dyalog",
    "APLCICD.VibeBenchmarks": "./src/VibeBenchmarks.dyalog",
    "APLCICD.ProjectLoader": "./src/ProjectLoader.dyalog",
    "APLCICD.HTMLDashboard": "./src/HTMLDashboard.dyalog"
  },
  "api": {
    "entry_points": [
      "APLCICD.Initialize",
      "APLCICD.TestCI",
      "APLCICD.SelfOptimize",
      "APLCICD.StartDashboard",
      "APLCICD.Vibe.Compress",
      "APLCICD.Vibe.Decompress",
      "APLCICD.Dashboard.GenerateUnifiedDashboard",
      "APLCICD.Pipeline.ProcessGitChanges",
      "APLCICD.AnalyzeProject",
      "APLCICD.ExternalDemo",
      "VibeBenchmarks.RunComprehensiveBenchmarks",
      "ProjectLoader.AnalyzeProject"
    ],
    "web_endpoints": [
      "/",
      "/api/vibe/status",
      "/api/vibe/toggle",
      "/api/vibe/compress",
      "/api/vibe/benchmarks",
      "/api/vibe/metrics",
      "/api/git/info",
      "/api/system/live",
      "/api/metrics",
      "/api/metrics/live",
      "/api/metrics/history",
      "/api/selfoptimize",
      "/api/status",
      "/api/git/status",
      "/api/git/log",
      "/api/git/commit",
      "/api/logs/commits",
      "/api/logs/optimization",
      "/api/project/analyze",
      "/webhook"
    ]
  },
  "config": {
    "default_config": "./config/default.json",
    "log_level": "info",
    "web_port": 8081,
    "auto_start_dashboard": true,
    "vibe_compression": true
  },
  "files": {
    "include": [
      "src/*.dyalog",
      "config/*.json",
      "web/*.html",
      "README.md",
      "CLAUDE.md"
    ],
    "exclude": [
      "*.tmp",
      "*.log",
      "*.bak",
      "aplcore"
    ]
  },
  "scripts": {
    "start": "./aplcicd complete",
    "demo": "./aplcicd demo",
    "dashboard": "./aplcicd dashboard",
    "test": "./aplcicd pipeline",
    "benchmark": "./aplcicd benchmark",
    "ai": "./aplcicd ai",
    "status": "./aplcicd status"
  },
  "keywords": [
    "CI/CD", 
    "APL", 
    "Vibe Coding", 
    "LLM Co-Creation", 
    "Token Compression", 
    "Self-Improving", 
    "Real-Time Analytics", 
    "GitHub Integration", 
    "WebHooks", 
    "Meta-Programming",
    "Interactive Dashboard",
    "Auto-Start Web Server",
    "Dyalog APL"
  ],
  "homepage": "https://github.com/jcfield-boop/aplipeline",
  "repository": {
    "type": "git",
    "url": "https://github.com/jcfield-boop/aplipeline.git"
  },
  "contest": {
    "name": "Dyalog APL Forge Contest 2025",
    "category": "Vibe Coding",
    "theme": "Self-Improving Systems",
    "features": [
      "Vibe coding compression (measured compression varies by content)",
      "LLM co-creation optimization", 
      "Working interactive dashboard with real Git integration",
      "Real CI/CD pipeline execution with audit logging", 
      "Self-optimization with actual analysis results",
      "File-based dashboard serving with live data",
      "Interactive vibe compression toggle via config.json",
      "Real SelfOptimizer with performance/quality scoring",
      "Production-ready architecture with persistent logging",
      "Zero-mock implementation - all functionality real including Git commits",
      "External APL project analysis with comprehensive reporting",
      "Webhook security with HMAC-SHA256 verification", 
      "Enhanced vibe coding demo with compression showcase",
      "Unified Pipeline and Monitor modules (merged architecture)",
      "Standardized error handling with specific trap codes",
      "Clean startup without HTMLRenderer dependencies",
      "GitHub webhook handler for automated CI/CD triggers"
    ],
    "innovations": [
      "Vibe coding approach to APL compression for LLM co-creation",
      "Unified module architecture eliminating code duplication",
      "Enhanced demonstration with compression showcase", 
      "GitHub webhook integration for automated pipeline triggers",
      "Standardized error handling across all system components",
      "Clean architecture without external renderer dependencies"
    ]
  },
  "performance": {
    "vibe_compression_ratio": "measured compression varies by content",
    "token_efficiency": "token savings demonstrated per pipeline",
    "architecture": "unified modules eliminate duplication", 
    "error_handling": "standardized :Trap 11 22 16 pattern",
    "reliability": "clean startup without HTMLRenderer dependencies",
    "demonstration": "vibe coding showcase with visual impact",
    "webhook_integration": "complete GitHub webhook handler",
    "pipeline_execution": "real file processing and validation",
    "git_integration": "native APL implementation with real commits"
  },
  "production": {
    "stability": "production-ready",
    "monitoring": "real-time with persistent logs",
    "error_handling": "comprehensive with audit trails",
    "scalability": "horizontal",
    "logging": "persistent audit trails for all operations",
    "git_integration": "native APL with dashboard controls"
  }
}