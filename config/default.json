{
  "pipeline": {
    "ai_threshold": 0.3,
    "quality_threshold": 0.5,
    "security_strict": true,
    "syntax_required": true,
    "parallel_workers": 4,
    "timeout_seconds": 300
  },
  "files": {
    "include_patterns": ["*.dyalog", "*.apl"],
    "exclude_patterns": ["*.tmp", "*.bak", "*.log"],
    "max_file_size_mb": 10,
    "encoding": "UTF-8"
  },
  "monitoring": {
    "enabled": true,
    "log_level": "info",
    "max_log_entries": 10000,
    "export_format": "json"
  },
  "security": {
    "validate_inputs": true,
    "webhook_verification": true,
    "webhook_secret": "aplcicd_secure_webhook_secret_2025",
    "rate_limit_requests": 100,
    "rate_limit_window_minutes": 60
  },
  "performance": {
    "cache_enabled": true,
    "cache_ttl_minutes": 30,
    "memory_limit_mb": 512,
    "gc_threshold": 1000
  },
  "vibe": {
    "enabled": true,
    "vibe_mode": true,
    "compression_level": "maximum",
    "auto_glossary": true,
    "target_compression": 0.7,
    "runtime_mode": false,
    "edit_mode": true,
    "preserve_comments": true,
    "comment_archive": "config/COMMENTS.archive",
    "compress_all_source": true,
    "source_patterns": ["src/*.dyalog", "src/*.apl"],
    "excluded_from_compression": [],
    "llm_development_mode": false
  },
  "metadata": {
    "version": "2.0.0",
    "config_format": "aplcicd_v2"
  }
}